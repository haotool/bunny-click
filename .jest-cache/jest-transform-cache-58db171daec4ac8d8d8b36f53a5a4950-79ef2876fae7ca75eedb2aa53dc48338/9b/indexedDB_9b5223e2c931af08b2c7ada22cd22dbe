exports.IndexedDBAdapter = void 0;
/**
 * IndexedDB ÈÅ©ÈÖçÂô®
 * Êèê‰æõÊ®ôÊ∫ñÂåñÁöÑ IndexedDB ‰ªãÈù¢ÔºåÊîØÊè¥Â§ßÂÆπÈáèÂÑ≤Â≠ò
 * 
 * @author haotool
 * @version 7.2.3
 * @created 2025-08-18T02:39:58+08:00
 */

class IndexedDBAdapter {
  constructor(config = {}) {
    this.config = {
      dbName: 'Bunny ClickDB',
      version: 1,
      storeName: 'gameData',
      ...config
    };
    this.db = null;
    this.isAvailable = this.checkAvailability();
  }

  /**
   * Ê™¢Êü• IndexedDB ÂèØÁî®ÊÄß
   */
  checkAvailability() {
    try {
      return 'indexedDB' in window && window.indexedDB !== null && window.indexedDB !== undefined;
    } catch (e) {
      console.warn('‚ö†Ô∏è IndexedDB ‰∏çÂèØÁî®:', e);
      return false;
    }
  }

  /**
   * ÂàùÂßãÂåñ IndexedDB
   */
  async init() {
    if (!this.isAvailable) {
      throw new Error('IndexedDB ‰∏çÂèØÁî®');
    }
    return new Promise((resolve, reject) => {
      const request = indexedDB.open(this.config.dbName, this.config.version);
      request.onerror = () => {
        reject(new Error(`IndexedDB ÈñãÂïüÂ§±Êïó: ${request.error?.message}`));
      };
      request.onsuccess = () => {
        this.db = request.result;
        this.log(`‚úÖ IndexedDB ÈÄ£Êé•ÊàêÂäü: ${this.config.dbName}`);

        // Ë®≠ÂÆöÈåØË™§ËôïÁêÜ
        this.db.onerror = event => {
          console.error('‚ùå IndexedDB Êìç‰ΩúÈåØË™§:', event);
        };
        resolve();
      };
      request.onupgradeneeded = event => {
        const db = event.target.result;
        this.log(`üîÑ IndexedDB ÂçáÁ¥ö: v${event.oldVersion} ‚Üí v${event.newVersion}`);

        // Âª∫Á´ã object store
        if (!db.objectStoreNames.contains(this.config.storeName)) {
          const store = db.createObjectStore(this.config.storeName, {
            keyPath: 'key'
          });

          // Âª∫Á´ãÁ¥¢Âºï
          store.createIndex('timestamp', 'timestamp', {
            unique: false
          });
          store.createIndex('expires', 'expires', {
            unique: false
          });
          this.log(`üì¶ Âª∫Á´ã Object Store: ${this.config.storeName}`);
        }
      };
    });
  }

  /**
   * Áç≤Âèñ‰∫§Êòì
   */
  getTransaction(mode = 'readonly') {
    if (!this.db) {
      throw new Error('IndexedDB Êú™ÂàùÂßãÂåñ');
    }
    return this.db.transaction([this.config.storeName], mode);
  }

  /**
   * Áç≤Âèñ Object Store
   */
  getStore(mode = 'readonly') {
    const transaction = this.getTransaction(mode);
    return transaction.objectStore(this.config.storeName);
  }

  /**
   * ÂÑ≤Â≠òË≥áÊñô
   */
  async setItem(key, value, options = {}) {
    if (!this.db) {
      throw new Error('IndexedDB Êú™ÂàùÂßãÂåñ');
    }
    return new Promise((resolve, reject) => {
      try {
        const store = this.getStore('readwrite');

        // Âª∫Á´ãË≥áÊñôÁâ©‰ª∂
        const data = {
          key,
          value,
          timestamp: Date.now(),
          version: this.config.version
        };

        // Ê∑ªÂä†ÈÅéÊúüÊôÇÈñì (Â¶ÇÊûúÊåáÂÆö)
        if (options.ttl) {
          data.expires = Date.now() + options.ttl;
        }
        const request = store.put(data);
        request.onsuccess = () => {
          this.log(`üíæ IndexedDB ÂÑ≤Â≠ò: ${key}`);
          resolve();
        };
        request.onerror = () => {
          reject(new Error(`IndexedDB ÂÑ≤Â≠òÂ§±Êïó: ${request.error?.message}`));
        };
      } catch (error) {
        reject(error);
      }
    });
  }

  /**
   * ËÆÄÂèñË≥áÊñô
   */
  async getItem(key) {
    if (!this.db) {
      throw new Error('IndexedDB Êú™ÂàùÂßãÂåñ');
    }
    return new Promise((resolve, reject) => {
      try {
        const store = this.getStore('readonly');
        const request = store.get(key);
        request.onsuccess = () => {
          const result = request.result;
          if (!result) {
            resolve(null);
            return;
          }

          // Ê™¢Êü•ÊòØÂê¶ÈÅéÊúü
          if (result.expires && Date.now() > result.expires) {
            // Áï∞Ê≠•ÁßªÈô§ÈÅéÊúüË≥áÊñô
            this.removeItem(key).catch(error => {
              this.log(`‚ö†Ô∏è ÁßªÈô§ÈÅéÊúüË≥áÊñôÂ§±Êïó: ${error.message}`);
            });
            this.log(`‚è∞ Ë≥áÊñôÂ∑≤ÈÅéÊúü: ${key}`);
            resolve(null);
            return;
          }
          this.log(`üìñ IndexedDB ËÆÄÂèñ: ${key}`);
          resolve(result.value);
        };
        request.onerror = () => {
          reject(new Error(`IndexedDB ËÆÄÂèñÂ§±Êïó: ${request.error?.message}`));
        };
      } catch (error) {
        reject(error);
      }
    });
  }

  /**
   * ÁßªÈô§Ë≥áÊñô
   */
  async removeItem(key) {
    if (!this.db) {
      throw new Error('IndexedDB Êú™ÂàùÂßãÂåñ');
    }
    return new Promise((resolve, reject) => {
      try {
        const store = this.getStore('readwrite');
        const request = store.delete(key);
        request.onsuccess = () => {
          this.log(`üóëÔ∏è IndexedDB ÁßªÈô§: ${key}`);
          resolve();
        };
        request.onerror = () => {
          reject(new Error(`IndexedDB ÁßªÈô§Â§±Êïó: ${request.error?.message}`));
        };
      } catch (error) {
        reject(error);
      }
    });
  }

  /**
   * Ê∏ÖÁ©∫ÊâÄÊúâË≥áÊñô
   */
  async clear() {
    if (!this.db) {
      throw new Error('IndexedDB Êú™ÂàùÂßãÂåñ');
    }
    return new Promise((resolve, reject) => {
      try {
        const store = this.getStore('readwrite');
        const request = store.clear();
        request.onsuccess = () => {
          this.log('üßπ IndexedDB Ê∏ÖÁ©∫ÂÆåÊàê');
          resolve();
        };
        request.onerror = () => {
          reject(new Error(`IndexedDB Ê∏ÖÁ©∫Â§±Êïó: ${request.error?.message}`));
        };
      } catch (error) {
        reject(error);
      }
    });
  }

  /**
   * Áç≤ÂèñÊâÄÊúâÈçµÂêç
   */
  async keys() {
    if (!this.db) {
      throw new Error('IndexedDB Êú™ÂàùÂßãÂåñ');
    }
    return new Promise((resolve, reject) => {
      try {
        const store = this.getStore('readonly');
        const request = store.getAllKeys();
        request.onsuccess = () => {
          resolve(request.result || []);
        };
        request.onerror = () => {
          reject(new Error(`IndexedDB Áç≤ÂèñÈçµÂêçÂ§±Êïó: ${request.error?.message}`));
        };
      } catch (error) {
        reject(error);
      }
    });
  }

  /**
   * Áç≤ÂèñÂÑ≤Â≠òÈ†ÖÁõÆÊï∏Èáè
   */
  async size() {
    if (!this.db) {
      throw new Error('IndexedDB Êú™ÂàùÂßãÂåñ');
    }
    return new Promise((resolve, reject) => {
      try {
        const store = this.getStore('readonly');
        const request = store.count();
        request.onsuccess = () => {
          resolve(request.result || 0);
        };
        request.onerror = () => {
          reject(new Error(`IndexedDB Ë®àÊï∏Â§±Êïó: ${request.error?.message}`));
        };
      } catch (error) {
        reject(error);
      }
    });
  }

  /**
   * Ê∏ÖÁêÜÈÅéÊúüË≥áÊñô
   */
  async cleanupExpiredData() {
    if (!this.db) {
      return;
    }
    return new Promise((resolve, reject) => {
      try {
        const store = this.getStore('readwrite');
        const index = store.index('expires');
        const now = Date.now();

        // Êü•Ë©¢ÊâÄÊúâÈÅéÊúüÁöÑË≥áÊñô
        const range = IDBKeyRange.upperBound(now);
        const request = index.openCursor(range);
        let deletedCount = 0;
        request.onsuccess = event => {
          const cursor = event.target.result;
          if (cursor) {
            // Âà™Èô§ÈÅéÊúüË≥áÊñô
            const deleteRequest = cursor.delete();
            deleteRequest.onsuccess = () => {
              deletedCount++;
            };
            cursor.continue();
          } else {
            // ÈÅçÊ≠∑ÂÆåÊàê
            if (deletedCount > 0) {
              this.log(`üßπ Ê∏ÖÁêÜÈÅéÊúüË≥áÊñô: ${deletedCount} È†ÖÁõÆ`);
            }
            resolve(deletedCount);
          }
        };
        request.onerror = () => {
          reject(new Error(`Ê∏ÖÁêÜÈÅéÊúüË≥áÊñôÂ§±Êïó: ${request.error?.message}`));
        };
      } catch (error) {
        reject(error);
      }
    });
  }

  /**
   * Áç≤Âèñ‰ΩøÁî®Áµ±Ë®à
   */
  async getStats() {
    try {
      const keys = await this.keys();
      const itemCount = keys.length;

      // ‰º∞ÁÆóÂ≠òÂÑ≤Á©∫Èñì‰ΩøÁî® (IndexedDB Ê≤íÊúâÁõ¥Êé•ÁöÑÂ§ßÂ∞èÊü•Ë©¢ÊñπÊ≥ï)
      // ÈÄôÊòØ‰∏ÄÂÄãÁ≤óÁï•ÁöÑ‰º∞ÁÆó
      let estimatedSize = 0;

      // ÂèñÊ®£ÈÉ®ÂàÜË≥áÊñô‰æÜ‰º∞ÁÆóÂπ≥ÂùáÂ§ßÂ∞è
      const sampleSize = Math.min(10, itemCount);
      if (sampleSize > 0) {
        const sampleKeys = keys.slice(0, sampleSize);
        let sampleTotalSize = 0;
        for (const key of sampleKeys) {
          const value = await this.getItem(key);
          if (value !== null) {
            const serialized = JSON.stringify(value);
            sampleTotalSize += serialized.length * 2; // UTF-16
          }
        }
        const averageSize = sampleTotalSize / sampleSize;
        estimatedSize = averageSize * itemCount;
      }
      return {
        itemCount,
        estimatedSize,
        formattedSize: this.formatBytes(estimatedSize),
        isEstimated: true
      };
    } catch (error) {
      this.log('‚ö†Ô∏è Áç≤ÂèñÁµ±Ë®àË≥áË®äÊôÇÁôºÁîüÈåØË™§:', error);
      return null;
    }
  }

  /**
   * Âü∑Ë°åË≥áÊñôÂ∫´Á∂≠Ë≠∑
   */
  async maintenance() {
    try {
      this.log('üîß ÈñãÂßã IndexedDB Á∂≠Ë≠∑...');

      // Ê∏ÖÁêÜÈÅéÊúüË≥áÊñô
      const deletedCount = await this.cleanupExpiredData();

      // Áç≤ÂèñÁµ±Ë®àË≥áË®ä
      const stats = await this.getStats();
      this.log('‚úÖ IndexedDB Á∂≠Ë≠∑ÂÆåÊàê', {
        deletedExpired: deletedCount,
        totalItems: stats?.itemCount || 0,
        estimatedSize: stats?.formattedSize || 'Êú™Áü•'
      });
      return {
        deletedExpired: deletedCount,
        stats
      };
    } catch (error) {
      this.log('‚ö†Ô∏è IndexedDB Á∂≠Ë≠∑ÊôÇÁôºÁîüÈåØË™§:', error);
      throw error;
    }
  }

  /**
   * ÈóúÈñâË≥áÊñôÂ∫´ÈÄ£Êé•
   */
  close() {
    if (this.db) {
      this.db.close();
      this.db = null;
      this.log('üîå IndexedDB ÈÄ£Êé•Â∑≤ÈóúÈñâ');
    }
  }

  /**
   * Ê†ºÂºèÂåñ‰ΩçÂÖÉÁµÑÂ§ßÂ∞è
   */
  formatBytes(bytes) {
    if (bytes === 0) return '0 Bytes';
    const k = 1024;
    const sizes = ['Bytes', 'KB', 'MB', 'GB'];
    const i = Math.floor(Math.log(bytes) / Math.log(k));
    return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];
  }

  /**
   * Ë™øË©¶Êó•Ë™å
   */
  log(...args) {
    if (this.config.debug) {
      console.log('üóÑÔ∏è IndexedDBAdapter:', ...args);
    }
  }
}
exports.IndexedDBAdapter = IndexedDBAdapter;
